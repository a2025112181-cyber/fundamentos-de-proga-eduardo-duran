#include <iostream>
#include <cmath>
using namespace std;

double POTEN(double x, double y) {
    return pow(x, y);
}

int main() {
    double x, y;
    cout << "Ingrese la base: ";
    cin >> x;
    cout << "Ingrese el exponente: ";
    cin >> y;
    cout << "El resultado de " << x << "^" << y << " es: " << POTEN(x, y) << endl;
    return 0;
}
